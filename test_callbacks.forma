// Test callback generation with when blocks

Button {
    text: "Click Me"
    x: 10
    y: 10
    width: 120
    height: 40
    
    when (clicked) {
        text: "Clicked!"
    }
}

Slider {
    value: 50
    min: 0
    max: 100
    x: 10
    y: 60
    
    when (value_changed) {
        label.text: value
    }
}

Label {
    text: "50"
    x: 150
    y: 70
}
